{
  "version": 2,
  "builds": [
    { "src": "myntra/package.json", "use": "@vercel/static-build" }
  ],
  "buildCommand": "cd myntra && npm ci && npx expo export --platform web",
  "outputDirectory": "myntra/dist",
  "routes": [
    {
      "src": "^/_expo/.*",
      "dest": "/_expo/$1"
    },
    {
      "src": "^/assets/.*",
      "dest": "/assets/$1"
    },
    {
      "src": "^/api/.*",
      "dest": "/api/$1"
    },
    {
      "src": "^/product/(.*)",
      "dest": "/product/$1"
    },
    {
      "src": "^\\(auth|tabs\\)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "^/(.*)\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)$",
      "dest": "/$1.$2"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
}